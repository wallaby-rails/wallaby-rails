<%# @param object [model] model instance %>
<%# @param field_name [String] name of the field %>
<%# @param value [Object] value of the field %>
<%# @param metadata [Hash] metadata of the field %>
<% if value.nil? %>
  <%= null %>
<% else %>
  <%= value ? fa_icon('check-square') : fa_icon('square') %>
<% end %>
